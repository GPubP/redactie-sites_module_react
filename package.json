  "description": "redactie sites module",
  "main": "dist/redactie-sites-module.umd.js",
  "module": "dist/redactie-sites-module.umd.js",
  "types": "dist/public/index.d.ts",
  "jsnext:main": "dist/redactie-sites-module.umd.js",
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:types && webpack --env.prod",
    "build:w": "npm run build:types && webpack -w --env.prod",
    "build:analyse": "npm run build:types && webpack --env.analyse",
    "start": "concurrently --kill-others \"npm run build:w\" \"cd demo && npm run start\"",
    "lint:scss": "stylelint public/**/*.scss --allow-empty-input",
    "lint:ts": "eslint --cache --cache-location .eslintcache --ext .ts,.tsx public",
    "lint": "npm run lint:scss && npm run lint:ts",
    "test": "jest ./public"
  },
  "repository": {
    "type": "git",
